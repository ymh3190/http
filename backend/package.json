  "scripts": {
    "db:rm": "docker compose -f ../docker-compose.yml rm dev-db -s -f -v",
    "db:up": "docker compose -f ../docker-compose.yml up dev-db -d",
    "db": "npm run db:rm && npm run db:up",
    "nginx:rm": "docker compose -f ../docker-compose.yml rm dev-nginx -s -f -v",
