<section id="product" class="page mes">
  <div class="content">
    <div class="top">
      <div class="header">
        <div class="left">
          <div class="wrapper-form">
            <form id="search" class="product">
              <%- include('../components/select', { id: 'item' ,inHtml: 'header', title: '종류' ,  values: [] }); %>
              <%- include('../components/input', { id: 'temp' ,inHtml: 'header', title: '제품명' }); %>
            </form>
          </div>
        </div>
        <div class="right">
          <%- include('../components/button', { id: 'search' , inHtml: 'header', title: '조회' ,  icon: ''  }); %>
