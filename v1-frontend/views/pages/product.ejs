            <div class="body">
              <div id="list" class="product"></div>
            </div>
          </div>
        </div>
        <div class="right">
          <div class="detail">
            <div class="top">
              <form id="saveUpdate" class="product">
                <%- include('../components/input', { id: 'name', inHtml: 'detail', title: '제품 명',  }); %>
              </form>
            </div>
            <div class="bottom">
              <%- include('../components/button', { id: 'create' ,inHtml: 'detail', title: '신규' ,  icon: '',  }); %>
              <%- include('../components/button', { id: 'save' ,inHtml: 'detail', title: '저장' , icon: '' ,  }); %>
