<script type="application/json" data-js="videos">
  [
    <% videos.forEach((video,i)=> { %>
    <% if (i<videos.length-1) { %> {
      "video": {
        "id": "<%= btoa(video.id) %>"
      }
    },
    <% return; %>
    <% } %> {
      "video": {
        "id": "<%= btoa(video.id) %>"
      }
    }
    <% }); %>
  ]
</script>