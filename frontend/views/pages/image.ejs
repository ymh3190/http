<section id="image" class="page tube">
  <div class="content">
    <div class="contents">
      <% for(let i=0;i<images.length;i++) { %>
      <% if (i<5) { %>
      <div class="container">
        <img src="<%= images[i].path %>" alt="">
      </div>
      <% continue; %>
      <% } %>
      <% break; %>
      <% } %>
      <%- include('../components/button', { id: 'add', inHtml: 'contents', title: '',  icon: 'fa-plus', }); %>
    </div>
  </div>
</section>