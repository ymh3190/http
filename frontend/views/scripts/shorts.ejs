<script type="application/json" data-js="shorts">
  [
    <% shorts.forEach((short,i)=> { %>
    <% if (i<shorts.length-1) { %> {
      "short": {
        "id": "<%= btoa(short.id) %>"
      }
    },
    <% return; %>
    <% } %> {
      "short": {
        "id": "<%= btoa(short.id) %>"
      }
    }
    <% }); %>
  ]
</script>